    "start": "expo start",
    "start:dev": "APP_ENV=development expo start",
    "start:prod": "APP_ENV=production expo start",
    "start:staging": "APP_ENV=staging expo start",
    "submit:android": "eas submit --platform android --profile production",
    "submit:android:staging": "eas submit --platform android --profile staging",
    "submit:ios": "eas submit --platform ios --profile production",
    "submit:ios:staging": "eas submit --platform ios --profile staging",
    "web": "expo start --web"
