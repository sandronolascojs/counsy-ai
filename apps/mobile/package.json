  "scripts": {
    "android": "expo run:android",
    "android:dev": "APP_ENV=development expo start --android",
    "android:prod": "APP_ENV=production expo start --android",
    "android:staging": "APP_ENV=staging expo start --android",
    "build:all:dev": "eas build --platform all --profile development",
    "build:android:dev": "eas build --platform android --profile development",
    "build:android:local": "eas build --platform android --profile development --local",
    "build:ios:dev": "eas build --platform ios --profile development",
    "build:ios:local": "eas build --platform ios --profile development --local",
    "devices:register": "eas device:create",
