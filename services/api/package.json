{
  "name": "@counsy-ai/api",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-p bundle type:gen",
    "bundle": "tsup-node",
    "bundle:watch": "tsup-node --watch",
    "clean": "del dist tsconfig.tsbuildinfo",
    "dev": "dotenv -e .env.local -- tsx --watch src/index.ts",
    "dev:local": "dotenv -e .env.local -- node ./dist/index.js",
    "format": "prettier --config ../../packages/config/.prettierrc --ignore-path ../../.prettierignore --write . && prettier-package-json --write ./package.json",
    "lint": "eslint src --max-warnings 0 --ext .ts,.tsx",
    "lint:fix": "eslint src --max-warnings 0 --fix --ext .ts,.tsx",
    "start": "node ./dist/index.js",
    "type:gen": "tsc --emitDeclarationOnly",
    "type:gen:watch": "tsc --emitDeclarationOnly --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "catalog:",
    "@better-auth/expo": "^1.3.5",
    "@counsy-ai/ai": "workspace:*",
    "@counsy-ai/config": "workspace:*",
    "@counsy-ai/db": "workspace:*",
    "@counsy-ai/shared": "workspace:*",
    "@counsy-ai/ts-rest": "workspace:*",
    "@counsy-ai/types": "workspace:*",
    "@fastify/aws-lambda": "catalog:",
    "@fastify/cors": "^11.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@paralleldrive/cuid2": "catalog:",
    "@slack/oauth": "^3.0.3",
    "@slack/web-api": "^7.9.3",
    "@t3-oss/env-core": "catalog:",
    "@ts-rest/core": "catalog:",
    "@ts-rest/fastify": "^3.52.1",
    "better-auth": "catalog:",
    "dotenv-cli": "catalog:",
    "drizzle-orm": "catalog:",
    "fastify": "catalog:",
    "fastify-plugin": "^5.0.1",
    "jose": "^6.1.0",
    "limax": "catalog:",
    "npm-run-all": "catalog:",
    "pg": "catalog:",
    "winston": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@paralleldrive/cuid2": "catalog:",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "prettier-package-json": "catalog:",
    "prettier-plugin-organize-imports": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  }
}
