{
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react-jsx",
    "types": [
      "node",
      "react",
      "react-dom"
    ],
    "moduleResolution": "bundler",
    "rootDir": "src",
    "baseUrl": ".",
    "composite": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules"
  ],
  "extends": "@counsy-ai/config/typescript-config/library.json",
  "include": [
    "src",
    "src/**/*.json"
  ]
}
